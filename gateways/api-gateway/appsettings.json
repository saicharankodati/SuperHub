{
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/auth"
          }
        ]
      },
      "dashboard": {
        "ClusterId": "dashboard-cluster",
        "Match": {
          "Path": "/dashboard/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/dashboard"
          }
        ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-destination": {
            "Address": "https://localhost:6661/"
          }
        }
      },
      "dashboard-cluster": {
        "Destinations": {
          "dashboard-destination": {
            "Address": "https://localhost:6662/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "FrontendOrigin": "http://localhost:3330"
  }
}
