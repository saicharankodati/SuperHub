{
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "dashboard": {
        "ClusterId": "dashboard-cluster",
        "Match": {
          "Path": "/dashboard/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-destination": {
            "Address": "http://localhost:6661/"
          }
        }
      },
      "dashboard-cluster": {
        "Destinations": {
          "dashboard-destination": {
            "Address": "http://localhost:6662/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
